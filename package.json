{
  "name": "outline-mcp-server",
  "version": "4.12.3",
  "description": "An MCP server for interacting with Outline's API",
  "type": "module",
  "bin": {
    "outline-mcp-server": "./build/index.js"
  },
  "files": [
    "build",
    "bin"
  ],
  "author": {
    "name": "Matt Frey",
    "email": "iam@mattfrey.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmmeff/outline-mcp-server"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "prepare": "npm run build",
    "watch": "bun --watch src/index.ts",
    "dev": "concurrently -n 'build,inspector' -c 'blue.bold,green.bold' 'npm run watch' 'npm run inspector'",
    "inspector": "npx @modelcontextprotocol/inspector@latest -y",
    "start": "bun build/index.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.12.1",
    "axios": "1.9.0",
    "bun": "^1.2.15",
    "dotenv": "16.5.0",
    "fastify": "^4.28.1",
    "omit-ts": "^2.0.1",
    "zod": "3.25.61"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "20.x",
    "concurrently": "^9.1.2",
    "prettier": "^3.2.5",
    "semantic-release": "^22.0.12",
    "typescript": "5.x"
  },
  "keywords": [
    "outline",
    "mcp",
    "getoutline.com",
    "model-context-protocol"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20"
  }
}
